{
  "name": "incubator-members",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "check": "turbo run build lint typecheck",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,jsonc}\"",
    "lint": "turbo run lint",
