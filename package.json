      "prettier --write"
    ]
  },
  "prettier": "@vercel/style-guide/prettier",
  "devDependencies": {
    "@vercel/style-guide": "^6.0.0",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.15.1",
  "engines": {
    "node": ">=18"
  }
