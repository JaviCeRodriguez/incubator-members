{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "site": {
      "packageName": "mfe-site",
      "development": {
        "local": 3000,
        "fallback": "microfrontends-marketing.labs.vercel.dev"
      }
    },
    "members": {
      "packageName": "mfe-members",
      "development": {
        "local": 3001
      },
      "routing": [
        {
          "group": "members",
          "paths": ["/members", "/members/:path*"]
        }
      ]
    }
  }
}
